#!/bin/sh

RPI_ROOT=$(realpath $(dirname $0)/..)

cp -r $RPI_ROOT/boot/* $RPI_ROOT/root/boot

cp -r $RPI_ROOT/../epipyweb $RPI_ROOT/root/tmp
cp -r $RPI_ROOT/../epipynet $RPI_ROOT/root/tmp

cp $RPI_ROOT/bin/emu-install $RPI_ROOT/root/tmp/emu-install
$RPI_ROOT/bin/rpi-chroot tmp/emu-install
killall /usr/bin/qemu-arm-static

rm -fr $RPI_ROOT/root/tmp/*
